<UserControl
    x:Class="DentalClinic.WpfMD.Views.ScheduleView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:c="clr-namespace:DentalClinic.WpfMD.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:v="clr-namespace:DentalClinic.WpfMD.Views"
    xmlns:vms="clr-namespace:DentalClinic.WpfMD.ViewModels"
    d:DataContext="{d:DesignInstance Type=vms:ScheduleViewModel}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <ListView>
            <c:CalendarView ItemBeginBinding="{Binding Visits, XPath=Date}" />

        </ListView>

    </Grid>
</UserControl>
